global ptr arr init with 6 x i32 [1, 2, 33, 4, 5, 6]
immutable global ptr N init with i32 -1
i32 main() {
entry:
  br bb %while.cond
while.cond:
  sum.phi = phi [bb %entry, i32 0, bb %while.body, i32 %14]
  i.phi = phi [bb %entry, i32 0, bb %while.body, i32 %16]
  7 = icmp slt i32 %i.phi, i32 6
  br i1 %7, bb %while.body, bb %while.exit
while.body:
  11 = mul i32 %i.phi, i32 4
  12 = ptradd ptr @arr, i32 %11
  12.load = load i32 from ptr %12
  14 = add i32 %sum.phi, i32 %12.load
  16 = add i32 %i.phi, i32 1
  br bb %while.cond
while.exit:
  ret i32 %sum.phi
}

