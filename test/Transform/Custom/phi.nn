global ptr value init with i32 0
external void putfarray(i32 , ptr )

i32 go() {
entry:
  br bb %while.cond
while.cond:
  a.phi = phi [bb %entry, i32 0, bb %while.body, i32 %10]
  value.load = load i32 from ptr @value
  7 = icmp slt i32 %a.phi, i32 %value.load
  br i1 %7, bb %while.body, bb %while.exit
while.body:
  10 = add i32 %a.phi, i32 1
  br bb %while.cond
while.exit:
  ret i32 %a.phi
}

